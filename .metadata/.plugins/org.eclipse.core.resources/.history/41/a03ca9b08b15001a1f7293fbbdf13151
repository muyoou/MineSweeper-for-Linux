package jblei;

public class Calculate {
	int[][] newCover;
	public Calculate(int wi,int he) {
		this.newCover=new int[wi][he];
	}
	public int[][] clear(int x,int y) {
		
		return newCover;
	}
	public static void main(String[] args) {
		MapManage myCreateMap =new MapManage(9, 9);
		Display myDisplay =new Display(myCreateMap.create(10), new int[9][9]);
		myDisplay.printHead();
		myDisplay.printMap();
		myDisplay.printFoot();
	}
}
